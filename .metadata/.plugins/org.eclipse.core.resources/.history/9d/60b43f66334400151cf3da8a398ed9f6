package com.sample.tictactoe;

import java.lang.reflect.Array;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

public class PlayGame {

	public void getuserMoveAndRespond(HttpServletRequest request, HttpServletResponse response){
		HttpSession session = request.getSession(true);
		String selectedpositions = null;
		  if (!(session.isNew())){
			  selectedpositions = (String) session.getAttribute("selectedPos");
		  }
		String[] arrayPos = selectedpositions.split(",");
		String userPositionString = request.getParameter("position");
		int userPosition = Integer.parseInt(userPositionString); 
		if (arrayPos.length >= 8){
			
			return;
		}
		
		
		
		
	}
}
